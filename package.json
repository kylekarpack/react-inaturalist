    "@vitejs/plugin-react": "^1.3.2",
    "gts": "^3.1.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4",
    "typescript-plugin-css-modules": "^3.4.0",
